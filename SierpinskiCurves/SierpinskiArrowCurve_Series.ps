


/level 1 def
/Inc_level { /level level 1 add def } def
/Dec_level { /level level 1 sub def } def

/inch { 72 mul } def

/baselength { 3 inch } def
/length { baselength } def



/UpRight_FlatRight_DownRight
{
    level 0 gt
    {
        Dec_level  
        FlatRight_UpRight_UpLeft  UpRight  UpRight_FlatRight_DownRight  FlatRight  DownLeft_DownRight_FlatRight  DownRight
        Inc_level 
    } if
} def



/FlatRight_UpRight_UpLeft
{
    level 0 gt
    {
        Dec_level  
        UpRight_FlatRight_DownRight  FlatRight  FlatRight_UpRight_UpLeft  UpRight  FlatLeft_UpLeft_UpRight  UpLeft
        Inc_level
    } if
} def


/FlatLeft_UpLeft_UpRight
{
    level 0 gt
    {
        Dec_level  
        UpLeft_FlatLeft_DownLeft FlatLeft  FlatLeft_UpLeft_UpRight UpLeft FlatRight_UpRight_UpLeft UpRight
        Inc_level
    } if
} def


/UpLeft_FlatLeft_DownLeft
{
    level 0 gt
    {
        Dec_level  
        FlatLeft_UpLeft_UpRight UpLeft UpLeft_FlatLeft_DownLeft FlatLeft DownRight_DownLeft_FlatLeft DownLeft 
        Inc_level
    } if
} def


/DownLeft_DownRight_FlatRight
{
    level 0 gt
    {
        Dec_level  
        DownRight_DownLeft_FlatLeft  DownLeft  DownLeft_DownRight_FlatRight  DownRight  UpRight_FlatRight_DownRight  FlatRight
        Inc_level
    } if
} def


/DownRight_DownLeft_FlatLeft
{
    level 0 gt
    {
        Dec_level
        DownLeft_DownRight_FlatRight DownRight  DownRight_DownLeft_FlatLeft DownLeft UpLeft_FlatLeft_DownLeft FlatLeft
        Inc_level
    } if
} def

 
/UpRight
{
    level 0 eq
    {
        length 2 div  length 2 div 3 sqrt mul  rlineto
    } if
} def


/DownRight
{
    level 0 eq
    {
        length 2 div  length 2 div 3 sqrt mul -1 mul  rlineto
    } if
} def


/FlatRight
{
    level 0 eq
    {
        length  0  rlineto
    } if
} def


/FlatLeft
{
    level 0 eq
    {
        length -1 mul  0  rlineto
    } if
} def


/UpLeft
{
    level 0 eq
    {
        length 2 div -1 mul  length 2 div 3 sqrt mul  rlineto
    } if
} def


/DownLeft
{
    level 0 eq
    {
        length 2 div -1 mul  length 2 div 3 sqrt mul -1 mul  rlineto
    } if
} def


/MakeSierpinkskiArrow
{
    gsave
        
        newpath

        /length baselength 2 level exp div def

        0 0 moveto
        UpRight_FlatRight_DownRight       

        stroke

    grestore

} def


1 setlinecap
2.5 setlinewidth
68 255 div 102 255 div 0 255 div setrgbcolor


/leftMargin { 8.5 inch baselength 2 mul sub 3 div } def
/bottomMargin { 11 inch  baselength 2 div 3 sqrt mul  3 mul  sub  4 div } def

gsave
    /level 1 def
    leftMargin   bottomMargin 3 mul  baselength 2 div 3 sqrt mul 2 mul  add translate
    MakeSierpinkskiArrow
grestore

gsave
    /level 2 def
    leftMargin 2 mul baselength add bottomMargin 3 mul  baselength 2 div 3 sqrt mul 2 mul  add translate
    MakeSierpinkskiArrow
grestore

gsave
    /level 3 def
    leftMargin  bottomMargin 2 mul  baselength 2 div 3 sqrt mul 1 mul  add translate
    MakeSierpinkskiArrow
grestore

gsave
    /level 4 def
    leftMargin 2 mul baselength add  bottomMargin 2 mul  baselength 2 div 3 sqrt mul 1 mul  add translate
    MakeSierpinkskiArrow
grestore

gsave
    /level 5 def
    leftMargin  bottomMargin baselength 2 div 3 sqrt mul 0 mul  add translate
    MakeSierpinkskiArrow
grestore

gsave
    /level 6 def
    leftMargin 2 mul baselength add  bottomMargin 1 mul  baselength 2 div 3 sqrt mul 0 mul  add translate
    MakeSierpinkskiArrow
grestore


showpage



